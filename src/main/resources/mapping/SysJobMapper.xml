<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.mukutech.websiteservice.mapper.SysJobMapper">

        <!-- 通用查询映射结果 -->
        <resultMap id="BaseResultMap" type="com.mukutech.websiteservice.pojo.entity.SysJob">
                    <id column="id" property="id"/>
